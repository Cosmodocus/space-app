{
  "extends": [
    "next/core-web-vitals",
    "next/typescript",
    "eslint:recommended",
    "prettier"
  ],
  "rules": {
    "prettier/prettier": ["error", { "trailingComma": "none" }],
    "no-unused-vars": ["warn", { "argsIgnorePattern": "^_" }],
    "@typescript-eslint/no-unused-vars": ["warn", { "argsIgnorePattern": "^_" }]
  },
  "plugins": [
    "prettier",
    "@typescript-eslint"
  ]
}
